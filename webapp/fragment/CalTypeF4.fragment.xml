<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <TableSelectDialog noDataText="{i18n>caltype.nodatatext}" title="{i18n>caltype.title}" growing="true" growingThreshold="100" confirm=".fnCaltypeSelect" search=".fnCaltypeSearch" items="{ path : 'oSrvModel>/ET_Calc_TypeSet' }" contentWidth="45%" resizable="true" draggable="true">
        <ColumnListItem>
            <cells>
                <ObjectIdentifier title="{oSrvModel>Calc_type}"/>
                <Text text="{oSrvModel>TXT}"/>
            </cells>
        </ColumnListItem>
        <columns>
            <Column width="12em">
                <header>
                    <Label text="{i18n>caltype.CalType}" design="Bold"/>
                </header>
            </Column>
            <Column width="12em">
                <header>
                    <Label text="{i18n>caltype.CalDesc}" design="Bold"/>
                </header>
            </Column>
        </columns>
    </TableSelectDialog>
</core:FragmentDefinition>

