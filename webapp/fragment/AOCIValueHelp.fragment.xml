<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <TableSelectDialog noDataText="{i18n>aocivalhelp.nodatatext}" title="{i18n>aocivalhelp.title}" growing="true" growingThreshold="100" confirm="fnAOCISelect" search="fnAOCISearch" items="{ path : 'oSrvModel>/ET_MaterialSet' }" contentWidth="50%">
        <ColumnListItem>
            <cells>
                <ObjectIdentifier title="{oSrvModel>Material}"/>
                <Text text="{oSrvModel>Text}"/>
            </cells>
        </ColumnListItem>
        <columns>
            <Column width="12em">
                <header>
                    <Label text="{i18n>aocivalhelp.ci}" design="Bold"/>
                </header>
            </Column>
            <Column width="12em">
                <header>
                    <Label text="{i18n>aocivalhelp.cidesc}" design="Bold"/>
                </header>
            </Column>
        </columns>
    </TableSelectDialog>
</core:FragmentDefinition>
