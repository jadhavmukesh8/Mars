<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <TableSelectDialog noDataText="{i18n>usrFields.nodatatext}" title="{i18n>usrFields.selCurrKey}" growing="true" growingThreshold="100" confirm="fnSelectCurKey" search="fnSearchCurKey" items="{ path : 'oSrvModel>/ET_Curr_KeySet' }" contentWidth="50%" resizable="true" draggable="true">
        <ColumnListItem>
            <cells>
                <ObjectIdentifier title="{oSrvModel>WAERS}"/>
                <Text text="{oSrvModel>LTEXT}"/>
            </cells>
        </ColumnListItem>
        <columns>
            <Column width="12em">
                <header>
                    <Label text="{i18n>usrFields.currKey}" design="Bold"/>
                </header>
            </Column>
            <Column width="12em">
                <header>
                    <Label text="{i18n>usrFields.currltext}" design="Bold"/>
                </header>
            </Column>
        </columns>
    </TableSelectDialog>
</core:FragmentDefinition>
